@using RentACar.Models
@{ 
    var imagePaths = (List<string>)ViewBag.ImagePaths;
}

<!-- banner -->
<div class="banner-w3ls" id="home">
    <!-- header -->
    <div class="header-w3l-agile">
        <div class="header_left">
            <ul>
                <li class="text-uppercase">@ApplicationWideData.ApplicationNameValue</li>
                <li><a href="mailto:@ApplicationWideData.EmailValue1"><span class="glyphicon glyphicon-envelope" aria-hidden="true"></span>@ApplicationWideData.EmailValue1</a></li>
                <li><span class="glyphicon glyphicon-earphone" aria-hidden="true"></span>@ApplicationWideData.PhoneNumberValue1</li>
            </ul>
        </div>
        <div class="header_right">
            <div class="w3ls-social-icons">
                <a class="facebook" href="@ApplicationWideData.FaceBookValue" target="_blank"><i class="fa fa-facebook"></i></a>
                <a class="twitter" href="#"><i class="fa fa-twitter"></i></a>
                <a class="pinterest" href="@ApplicationWideData.YouTubeValue" target="_blank"><i class="fa fa-youtube"></i></a>
                <a class="linkedin" href="#"><i class="fa fa-linkedin"></i></a>

            </div>

        </div>
        <div class="clearfix"></div>
    </div>
    <!-- //header -->
    <div class="container">
        <!-- .header-nav was removed from here -->
        <!-- .clearfix was removed from here -->
        <h2>@ApplicationWideData.ApplicationNameValue</h2>
        <h3>@ApplicationWideData.SubHeadingJumbotron</h3>
        
        <div id="owl-demo" class="owl-carousel">
            @foreach (var path in imagePaths)
            {
                <div class="item-owl">
                    <div class="test-review">
                        <img src="@Url.Content($"{ApplicationWideData.StorageRootPath}/{path}")" class="img-responsive" style="width:250px; height:200px" alt="" />
                    </div>
                </div>
            }
        </div>




    </div>
</div>
<!-- //banner -->
