@model IEnumerable<RentACar.Models.Sublocation>

@{
    ViewBag.Title = "SublocationList";
}

<h2>SublocationList</h2>

<p>
    @Html.ActionLink("Create New", "CreateSublocation")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BookingRequest.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BookingRequest.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "DeleteSublocation", new { id=item.Id })
        </td>
    </tr>
}

</table>
