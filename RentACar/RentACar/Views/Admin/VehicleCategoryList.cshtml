@model IEnumerable<RentACar.Models.VehicleCategory>

@{
    ViewBag.Title = "VehicleCategoryList";
}

<h2>VehicleCategoryList</h2>

<p>
    @Html.ActionLink("Create New", "CreateVehicleCategory")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "DeleteVehicleCategory", new { id=item.Id })
        </td>
    </tr>
}

</table>
